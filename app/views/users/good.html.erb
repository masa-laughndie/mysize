<% provide(:title, @user.name + "(@" + @user.mysize_id + ")さんのグッド") %>
<% provide(:og_title, @user.name + "(@" + @user.mysize_id + ")さんのグッド") %>
<% provide(:og_url, good_user_url(@user)) %>

<div class="container content-mspace">
  <%= render partial: 'users/user_profile',
             locals: { user: @user } %>
</div>
<div class="container post">
  <%= render partial: 'users/post_select',
             locals: { user: @user } %>
  <div class="post-main">
    <% if @goods.blank? %>
      <%= render partial: 'shared/no_data',
                 locals: { icon: 'good',
                           text: 'good' } %>
    <% else %>
      <ul>
        <%= render @goods %>
      </ul>
    <% end %>
  </div>
</div>